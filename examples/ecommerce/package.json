{
  "private": true,
  "name": "js-sdk-ecommerce-example",
  "version": "0.0.0",
  "license": "MIT",
  "browserslist": "> 1%",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "build:env": "node -r esm scripts/build-env.js",
    "build:prod": "cross-env NODE_ENV=production preact build",
    "build:netlify": "yarn build:env && preact build",
    "serve": "sirv build --cors --single",
    "dev": "preact watch --host=localhost",
    "analyze": "cross-env NODE_ENV=production preact build --analyze",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "devDependencies": {
    "@sampotts/eslint-config": "^1.0.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "if-env": "^1.0.4",
    "postcss": "^7.0.32",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^7.1.1",
    "preact-cli": "3.0.1",
    "sirv-cli": "^1.0.5",
    "webpack": "^4.44.1"
  },
  "dependencies": {
    "@sajari/sdk-js": "next",
    "@sajari-ui/core": "^0.1.136",
    "classnames": "^2.2.6",
    "downshift": "^6.0.3",
    "preact": "^10.4.7",
    "preact-render-to-string": "^5.1.10",
    "prop-types": "^15.7.2",
    "tailwindcss": "^1.6.2",
    "tailwindcss-gradients": "^3.0.0"
  }
}
